# Sofkify Frontend Environment Configuration
# Copy this file to .env or .env.development and update values as needed

# User Service (Port 8080)
# Used for: Login, Register, User profile operations
VITE_API_BASE_URL=http://localhost:8080

# Product Service (Port 8081)
# Used for: Product listing, product details
VITE_PRODUCT_SERVICE_URL=http://localhost:8081

# Cart Service (Port 8083)
# Used for: Cart operations (add items, update quantities, remove items)
VITE_CART_SERVICE_URL=http://localhost:8083

# Order Service (Port 8082)
# Used for: Order creation, order tracking, order status updates
VITE_ORDER_SERVICE_URL=http://localhost:8082
